var FORM_TYPE_BULKEDIT = 6;
var FORM_TYPE_READOPTIMIZED = 11; // Deprecated

var DEFAULT_COUNTRYNAME = "FRANCE";
var DEFAULT_COUNTRYCODE = "FR";

function Lookup(id, name, entityType) {
    var lookup = [];
    lookup[0] = {};
    lookup[0].id = id;
    lookup[0].name = name;
    lookup[0].entityType = entityType;

    return lookup;
}

